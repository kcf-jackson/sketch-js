{
  "name": "r-generic",
  "version": "1.0.0",
  "description": "A collection of functions providing R-like functionalities",
  "main": "R.js",
  "scripts": {
    "build": "yarn webpack -p",
    "build-dev": "yarn babel --presets=@babel/preset-env src --out-dir ./bin/modules/",
    "test-dev": "yarn jest --coverage=true ./tests/test-files/ && yarn test-coverage",
    "test-coverage": "open ./tests/test-coverage/index.html",
    "doc": "yarn documentation build ./src/ -f html -o ./docs/",
    "clean": "rm -r ./bin/dist/ && rm -r ./bin/modules/ && mkdir ./bin/dist && mkdir ./bin/modules",
    "watch": "webpack --watch"
  },
  "author": "Chun Fung Kwok",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-jest": "^26.0.1",
    "documentation": "^13.0.2",
    "jest": "^26.0.1",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "dataframe-js": "^1.4.3",
    "mathjs": "^7.0.0",
    "ramda": "^0.27.0"
  }
}
